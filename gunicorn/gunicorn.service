[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/django_project
ExecStart=/home/ubuntu/django_project/venv_django/bin/gunicorn \
    --access-logfile gunicorn/gunicorn-access.log \
    --error-logfile gunicorn/gunicorn-error.log \
    --workers 3 \
    --bind unix:/home/ubuntu/django_project/gunicorn/django.sock \
    sus_3_live.wsgi:application

[Install]
WantedBy=multi-user.target